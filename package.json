{
  "name": "todo",
  "version": "0.0.1",
  "description": "Client for todo-app",
  "scripts": {
    "pom": "shadow-cljs pom",
    "dev": "shadow-cljs watch app",
    "build-css": "postcss resources/public/css/pre-app-styles.css -o resources/public/css/output/app-styles.css",
    "watch-css": "postcss resources/public/css/pre-app-styles.css -o resources/public/css/output/app-styles.css --watch",
    "release": "shadow-cljs release app",
    "start": "shadow-cljs server",
    "clean": "rm -rf resources/public/js; rm -rf resources/public/css/output"
  },
  "dependencies": {
    "@nivo/core": "^0.66.0",
    "@nivo/pie": "^0.66.0",
    "autoprefixer": "^10.0.4",
    "create-react-class": "^15.7.0",
    "highlight.js": "9.18.1",
    "postcss": "^8.1.10",
    "postcss-cli": "^8.3.0",
    "postcss-import": "^13.0.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-flip-move": "3.0.4",
    "react-highlight.js": "1.0.7",
    "tailwindcss": "^2.0.3"
  },
  "devDependencies": {
    "shadow-cljs": "^2.11.8"
  }
}
